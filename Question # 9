<!DOCTYPE html>
<html>
    <head>
        <title>Fahrenheit to Celsius Converter</title>
    </head>
    <body>
        <div id="output"></div>
<script>
function convertFahrenheitToCelsius(fahrenheit) {
    let celsius = (fahrenheit - 32) * 5 / 9; 
    return celsius; 
}
let fahrenheitValue = 75.325; 
let celsiusValue = convertFahrenheitToCelsius(fahrenheitValue); 

document.getElementById("output").innerHTML = fahrenheitValue + "° F is equal to " + celsiusValue.toFixed(2) + "° C"; 
</script>
    </body>
</html>
