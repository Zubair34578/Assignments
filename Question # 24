<!DOCTYPE html>
<html>
<head>
    <title>Merge Two Arrays</title>
</head>
<body>

    <h1>Merge Two Arrays</h1>
 <p id="result"></p>
 <script>
        function mergeArrays(arr1, arr2) {
            let mergedArray = [];      
   for (let i = 0; i < arr1.length; i++) {
   mergedArray.push(arr1[i]);
     }   
  for (let j = 0; j < arr2.length; j++) {
   mergedArray.push(arr2[j]);
    }

  return mergedArray;
   }
 let array1 = [1, 2, 3];
 let array2 = [4, 5, 6];
 let resultArray = mergeArrays(array1, array2);

        document.getElementById('result').textContent = 'Merged Array: ' + resultArray;
    </script>

</body>
</html>
