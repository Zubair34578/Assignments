<!DOCTYPE html>
<html>
    <head>
        <title>Average Of Array Numbers</title>
    </head>
    <body>
        <div id="output"></div>
<script>
let numbers = [1,2,3,4,5,6];
let sum=0;
for (let i=0; i < numbers.length; i++) {
    sum += numbers [i];
}
let average= sum / numbers.length;
document.getElementById("output").innerHTML = "The average of numbers is:" + average;

</script>
    </body>
</html>
