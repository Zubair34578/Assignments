<!DOCTYPE html>
<html>
<head>
    <title>Find Nth Fibonacci Number</title>
</head>
<body>
    <div id="output"></div>
<script>
function findFibonacci(n) {
  let n1 = 0, n2 = 1, nextTerm;

if (n === 0) {
    return n1;
} else if (n === 1) {
 return n2;
}
for (let i = 2; i <= n; i++) {
nextTerm = n1 + n2;  
n1 = n2;  
n2 = nextTerm;
}
return n2; 
}
let nthFibonacci = 19;
    let fibonacciNumber = findFibonacci(nthFibonacci);
document.getElementById("output").innerHTML = "The " + nthFibonacci + "th Fibonacci Number is: " + fibonacciNumber;
</script>
</body>
</html>
